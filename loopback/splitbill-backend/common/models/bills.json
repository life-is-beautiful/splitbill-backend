{
  "name": "Bills",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "description": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "amount": {
      "type": "number"
    },
    "tax": {
      "type": "number"
    },
    "discount": {
      "type": "number"
    },
    "tip": {
      "type": "number"
    },
    "serviceCharge": {
      "type": "number"
    },
    "status": {
      "type": "string",
      "comments": "(Active / Inactive)"
    }
  },
  "validations": [],
  "relations": {
    "items": {
      "type": "hasMany",
      "model": "Items",
      "foreignKey": "billId"
    },
    "payments": {
      "type": "hasMany",
      "model": "Payments",
      "foreignKey": "billId"
    }
  },
  "acls": [],
  "methods": {}
}
