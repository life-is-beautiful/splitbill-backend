{
  "name": "Customers",
  "base": "User",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nickname": {
      "type": "string"
    },
    "device": {
      "type": "string"
    },
    "fullname": {
      "type": "string"
    },
    "email": {
      "required": false
    },
    "password": {
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "bills": {
      "type": "hasMany",
      "model": "Bills",
      "foreignKey": "owedId"
    },
    "items": {
      "type": "hasMany",
      "model": "Items",
      "foreignKey": "owingId"
    },
    "owingpayments": {
      "type": "hasMany",
      "model": "Payments",
      "foreignKey": "owingId"
    },
    "owedpayments": {
      "type": "hasMany",
      "model": "Payments",
      "foreignKey": "owedId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
